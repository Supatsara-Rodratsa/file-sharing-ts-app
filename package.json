{
  "name": "file-sharing-app",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "export NODE_OPTIONS='--trace-deprecation --abort-on-uncaught-exception' && export NODE_ENV=development && nodemon --exec ts-node ./bin/www.ts",
